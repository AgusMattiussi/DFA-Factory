/* Declaramos las variables previamente */
sta initial = "q0";
sta final = "qf";
sta[] states = {initial, final};
sta[] finalStates = {final};
sym nums = "123456789";
sym[] symbols = {"a", "bb", nums};
trn[] transitions = {
					{initial, final, "a"},
					{final, initial, nums},
					{final, final, "bb"}
};

/* Declaramos el automata */
dfa myAuto = {
        states,
        symbols,
        initial,
        finalStates,
		transitions
};

trn newTransition = {initial, final, "bb"};

dfa myAuto2 = ADD newTransition TO myAuto;
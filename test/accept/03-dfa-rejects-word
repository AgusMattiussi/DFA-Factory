/* Declaramos las variables previamente */
sta inicial = "q0";
sta final = "qf";
sta[] states = {inicial, final};
sta[] finalStates = {final};
sym nums = "123456789";
sym[] symbols = {"a", "bb"};
trn[] transitions = {
					{inicial, final, "a"},
					{final, final, "bb"}
};

/* Declaramos el automata */
dfa myAuto = {
        states,
        symbols,
        inicial,
        finalStates,
				transitions
};
myauto CHECK {nums};
/* Declaramos las variables previamente */
sta initial = "q0";
sta final = "qf";
sta[] states = {inicial, final};
sta[] finalStates = {final};
sym nums = "123456789";
sym[] symbols = {"a", "bb", nums};
trn[] transitions = {
					{inicial, final, "a"},
					{final, inicial, nums},
					{final, final, "bb"}
};

/* Declaramos el automata */
dfa myAuto = {
        states,
        symbols,
        initial,
        finalStates,
				transitions
};

myAuto CHECK {"a", "bb", "bb", "bb"};
myAuto CHECK {"a", "bb", "bb", "bb"};
myAuto CHECK {"a", "bb", "bb", "c"};